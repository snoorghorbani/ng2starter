{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [      
      "es2017",
      "dom"
    ],
    "paths": {
      "@soushians/layout": [
        "dist/@soushians/layout"
        // "projects/soushians/layout/src/public_api"
      ],
      "@soushians/config": [
        "dist/@soushians/config"
      ],
      "@soushians/user": [
        "dist/@soushians/user"
      ],
      "@soushians/infra": [
        "dist/@soushians/infra"
      ],
      "@soushians/shared": [
        "dist/@soushians/shared"
      ],
      "@soushians/authentication": [
        "dist/@soushians/authentication"
      ],
      "@soushians/form": [
        "dist/@soushians/form"
      ],
      "@soushians/diagram": [
        "dist/@soushians/diagram"
      ],
      "@soushians/source": [
        "dist/@soushians/source"
      ],
      "@soushians/socket": [
        "dist/@soushians/socket",
        // "projects/soushians/socket/src/public_api"
      ],
      "@soushians/dashboard": [
        "dist/@soushians/dashboard"
      ],
      "@soushians/test": [
        "dist/soushians/test"
      ],
      "socket.io-client": ["./node_modules/socket.io-client/dist/socket.io.slim.js"],
      "@rule": [
        "src/app/rule"
      ]
    }
  }
}