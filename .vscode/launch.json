{
    // Use IntelliSense to learn about possible Node.js debug attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Main",
            "cwd": "E:/@ng2-starter/starter/db/",
            "program": "${workspaceFolder}/db/server.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Mongo do Backup",
            "program": "${workspaceFolder}/db/dobackup.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Mongo Version",
            "cwd": "E:/@ng2-starter/starter/db/",
            "program": "${workspaceFolder}/db/version.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Mongo Compare",
            "cwd": "E:/@ng2-starter/starter/db/",
            "program": "${workspaceFolder}/db/compare.js"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach",
            "port": 9229
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug",
            "program": "${workspaceRoot}/dist/server/server.js",
            "smartStep": true,
            "outFiles": [
                "../dist/**/*.js"
            ],
            // "preLaunchTask": "npm: build",
            "protocol": "inspector"
        }
    ]
}