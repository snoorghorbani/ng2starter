{
    "name": "@soushians/starter-server",
    "version": "6.6.0",
    "scripts": {
        "start": "npm run build && npm run watch",
        "build-o": "npm run build-ts && npm run tslint",
        "build": "npm run build-ts",
        "serve": "nodemon dist/server/server.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" -n \"npm run watch-ts\" \"npm run serve\"",
        "test": "jest --forceExit",
        "build-ts": "tsc ",
        "watch-ts": "tsc -w",
        "tslint": "tslint -c tslint.json",
        "copy-static-assets": "node ./scripts/copyStaticAssets.js",
        "debug": "npm run build && npm run watch-debug",
        "serve-debug": "nodemon --inspect dist/server/server.js",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" -n \"npm run watch-ts\" \"npm run serve-debug\"",
        "dbm_up": "node ./node_modules/mongodb-migrate -runmm -cfg db-migration.json -dbn starter up",
        "dbm_down": "node ./node_modules/mongodb-migrate -runmm -cfg db-migration.json -dbn starter down"
    },
    "dependencies": {
        "@types/async": "^2.0.47",
        "@types/bcrypt-nodejs": "^0.0.30",
        "@types/body-parser": "^1.16.2",
        "@types/compression": "0.0.33",
        "@types/connect-mongo": "0.0.32",
        "@types/cookie-parser": "^1.4.1",
        "@types/cors": "^2.8.3",
        "@types/dotenv": "^2.0.20",
        "@types/errorhandler": "0.0.30",
        "@types/express": "^4.0.35",
        "@types/express-session": "0.0.32",
        "@types/lodash": "^4.14.104",
        "@types/mongodb": "^3.0.5",
        "@types/mongoose": "^5.0.2",
        "@types/morgan": "^1.7.35",
        "@types/node": "^9.4.6",
        "@types/passport": "^0.4.3",
        "@types/passport-facebook": "^2.1.7",
        "@types/passport-local": "^1.0.32",
        "@types/request": "^2.47.0",
        "@types/socket.io": "^1.4.33",
        "@types/socket.io-client": "^1.4.32",
        "@types/supertest": "^2.0.4",
        "async": "^2.6.0",
